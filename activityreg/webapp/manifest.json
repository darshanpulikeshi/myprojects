{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.test.activityreg",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.6",
      "toolsId": "b03a7cb6-6b0a-4741-9ad4-dd6a55cb6360"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/northwind/northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "localJson": {
        "uri": "./model/Property.json",
        "type": "JSON"
      }
    },
    "crossNavigation": {
      "inbounds": {
        "melodymta": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "melodymta",
          "action": "display",
          "title": "Melody Activity Form"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.113.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.test.activityreg.i18n.i18n"
        }
      },
      "mDefaultODataModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "mCommon": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "localJson"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.test.activityreg.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "ActivityRegistration",
          "pattern": "",
          "target": ["TargetActivityRegistration"]
        },
        {
          "name": "AcadaemyRequest",
          "pattern": "AcadaemyRequest",
          "target": ["TargetAcadaemyRequest"]
        },
        {
          "name": "CXRFP",
          "pattern": "CXRFP",
          "target": ["TargetCXRFP"]
        },
        {
          "name": "CXDesign",
          "pattern": "CXDesign",
          "target": ["TargetCXDesign"]
        }
      ],
      "targets": {
        "TargetActivityRegistration": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ActivityRegistration",
          "viewName": "ActivityRegistration"
        },
        "TargetAcadaemyRequest": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AcadaemyRequest",
          "viewName": "AcadaemyRequest"
        },
        "TargetCXRFP": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CXRFP",
          "viewName": "CXRFP"
        },
        "TargetCXDesign": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CXDesign",
          "viewName": "CXDesign"
        }
      }
    },
    "rootView": {
      "viewName": "com.test.activityreg.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "melomanapp"
  },
  "sap.platform.cf": {
    "oAuthScopes": ["$XSAPPNAME.approver"]
  }
}
